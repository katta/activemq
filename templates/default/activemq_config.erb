export ACTIVEMQ_HOME="<%=@activemq_home%>"
export ACTIVEMQ_BASE="$ACTIVEMQ_HOME"
export ACTIVEMQ_CONF="<%=node['activemq']['conf_dir']%>"
export ACTIVEMQ_DATA="<%=node['activemq']['data_dir']%>"
export ACTIVEMQ_TMP="$ACTIVEMQ_BASE/tmp"
export ACTIVEMQ_PIDFILE="<%=node['activemq']['pid_dir']%>/activemq.pid"
export ACTIVEMQ_OPTS="-Xms<%=node['activemq']['wrapper']['min_memory']%> -Xmx<%=node['activemq']['wrapper']['max_memory']%> -Dorg.apache.activemq.UseDedicatedTaskRunner=<%=node['activemq']['wrapper']['useDedicatedTaskRunner']%>"